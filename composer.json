{
    "name": "pelim/laravel-zmq",
    "description": "zmq support for Laravel 5",
    "license": "MIT",
    "authors": [
        {
            "name": "pelim",
            "email": "peter.limbach@gmail.com"
        }
    ],
    "require": {
        "ext-zmq": "*",
        "illuminate/broadcasting": "5.5.*",
        "illuminate/contracts": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Pelim\\LaravelZmq\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pelim\\LaravelZmq\\ZmqServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}